{
  "name": "led-view",
  "version": "0.0.1",
  "description": "led-view",
  "scripts": {
    "bs-admin": "rm .bootstraprc && cp .bootstraprc-admin .bootstraprc",
    "bs-teacher": "rm .bootstraprc && cp .bootstraprc-teacher .bootstraprc",
    "start:a": "npm run bs-admin && node server-front.js",
    "start:t": "npm run bs-teacher && node server-teacher.js",
    "build": "npm run build:a && npm run build:t",
    "build:a": "npm run bs-admin && NODE_ENV=production webpack --bail --config webpack-admin/prod.config.js",
    "build:t": "npm run bs-teacher && NODE_ENV=production webpack --bail --config webpack-teacher/prod.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src-admin",
    "test": "NODE_ENV=test mocha 'test/UnitTest/**/*.js' --recursive --compilers js:babel-core/register --require ./test/UnitTest/setup.js --require ./test/UnitTest/css-null-compiler.js",
    "test:db": "./node_modules/.bin/babel-node test.js"
  },
  "license": "MIT",
